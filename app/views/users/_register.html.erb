<%= form_for @user, url: @create_user, class: "what-form", :remote => true do |f| %>
  <%= f.label :first_name %><br>
  <%= f.text_field :first_name %><br>
  <%= f.label :last_name %><br>
  <%= f.text_field :last_name %><br>



  <h2>Address:</h2>
  <%= f.label :street %><br>
  <%= f.text_field :street %><br>
  <%= f.label :city %><br>
  <%= f.text_field :city %><br>
  <%= f.label :state %><br>
  <%= f.text_field :state %><br>
  <%= f.label :zip %><br>
  <%= f.text_field :zip %><br>
  <%= f.label :phone %><br>
  <%= f.text_field :phone %><br>

  <h2>Password:</h2>
  <%= f.label :password %><br>
  <%= f.text_field :password %><br>
  <%= f.label :password_confirmation %><br>
  <%= f.text_field :password_confirmation %><br>
  <%= f.submit "Register", class: 'button' %>
<% end %>
