<div id="map"></div>
  <script>
      var locations = <%= @tennis_singles %>
      function initMap() {
        var san_francisco = {lat: 37.7845165, lng: -122.3971939};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 15,
          center: san_francisco
        });

        var marker = new google.maps.Marker({
          position: san_francisco,
          map: map
        });
      }
</script>
  <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=#{ENV[BLAH]}&callback=initMap">
  </script>
